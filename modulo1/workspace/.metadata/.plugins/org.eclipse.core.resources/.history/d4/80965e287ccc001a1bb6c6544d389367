package com.glovallogic.app;

public class TiposPrimitivos {
	

public static void main(String[] args) {
int myNum=15;
char caracter='a';
boolean bol=true;
float myNum2=(float) 2436.2456;
short myNum3=500;
System.out.println("El valor de mi numero es "+myNum);
System.out.println("El valor de mi caracter es "+caracter);
System.out.println("El valor de mi booleano es "+bol);
System.out.println("El valor de mi float es "+myNum2);
System.out.println("El valor de mi short es "+myNum3);
}
}

