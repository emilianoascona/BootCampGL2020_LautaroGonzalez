package ejer5;

public class Heladera extends Electrodomestico {
	private double capacidad;
	private boolean tieneFreezer;
	private double capacidadDelFreezer;
	
	public void GuardarElemento(double item) {
		if(capacidad<=0) {
			System.out.println("la heladera esta llena");
		}else
			capacidad=capacidad-item;
	}
}
