package ejer5;

public class testElectrodomesticos {
	public static void main(String[] args) {
		Electrodomestico heladera= new Heladera();
		Electrodomestico televisor= new Televisor();
		Electrodomestico cafetera= new Cafetera();
		Electrodomestico[] vecElectro= new Electrodomestico[] {heladera,televisor,cafetera};
		for(int i=0;i<vecElectro.length;i++) {
			vecElectro[i].apagar();
		}
	}
}
